        {
          "datasource": { "type": "influxdb", "uid": "${datasource}" },
          "query": "// Try to get real-time session cost from session_state (during active charging)\nsessionCost = from(bucket: \"twc_dashboard\")\n  |> range(start: -5m)\n  |> filter(fn: (r) => r._measurement == \"twc_session_state\")\n  |> filter(fn: (r) => r._field == \"full_cost_dollars\")\n  |> filter(fn: (r) => r.charger_id == \"${charger}\")\n  |> last()\n\n// Fallback: estimate from vitals session_energy_wh if no session_state\nfallback = from(bucket: \"twc_dashboard\")\n  |> range(start: -5m)\n  |> filter(fn: (r) => r._measurement == \"twc_vitals\")\n  |> filter(fn: (r) => r._field == \"session_energy_wh\")\n  |> filter(fn: (r) => r.charger_id == \"${charger}\")\n  |> last()\n  |> map(fn: (r) => ({r with _value: (r._value / 1000.0) * ((5.0 + ${delivery_rate}) / 100.0), _measurement: \"twc_session_state\", _field: \"full_cost_dollars\"}))\n\nunion(tables: [sessionCost, fallback])\n  |> sort(columns: [\"_time\"], desc: true)\n  |> first()\n  |> keep(columns: [\"_value\"])",
          "refId": "A"
        }
      ],
      "title": "Session Cost",
      "description": "Real-time session cost (accurate during charging, estimated otherwise)",
      "type": "stat"
