          "datasource": { "type": "influxdb", "uid": "${datasource}" },
          "query": "from(bucket: \"twc_dashboard\")\n  |> range(start: -48h)\n  |> filter(fn: (r) => r._measurement == \"comed_meter_usage\")\n  |> filter(fn: (r) => r._field == \"kwh\")\n  |> count()\n  |> map(fn: (r) => ({r with _value: if r._value > 0 then 1 else 0}))\n  |> keep(columns: [\"_value\"])",
