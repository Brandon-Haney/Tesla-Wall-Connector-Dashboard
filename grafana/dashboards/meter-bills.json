      "description": "Shows Connected if Opower data was received in the last 48 hours",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [
            { "options": { "1": { "color": "green", "index": 0, "text": "Connected" } }, "type": "value" },
            { "options": { "0": { "color": "red", "index": 1, "text": "Expired" } }, "type": "value" }
